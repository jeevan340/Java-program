import java.io.*;
import java.util.*;
public class Reverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the string:");
		String j=s.nextLine();
		int l,i;
		char ch;
		String j3="";
		for(i=j.length();i>0;i--)
		{
			ch=j.charAt(i-1);
			j3=j3+ch;
		}
		System.out.println(j3);

	}

}
