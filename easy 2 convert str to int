import java.io.*;
import java.util.*;
public class Strtoint {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("enter the string:");
		String j=s.nextLine();
		int i=Integer.parseInt(j);
		System.out.println(i);
	}

}
